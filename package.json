{
  "name": "JATE",
  "version": "1.0.0",
  "description": "Just Another Text Editor",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
"start:dev": "nodemon src/server.js",
"server": "node src/server.js",
    "build": "echo 'No build step required' || true",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run start:dev\" \"npm run client\""
  },
  "keywords": ["text-editor", "web-app"],
  "author": "Marisol Aranda",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}

